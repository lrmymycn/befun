			<result name="success" type="myJson">
				<param name="includeProperties">
					pgb\..*,
					aggRs.*,
				</param>
				<param name="excludeProperties">
					pgb\.models.*model,
					aggRs\[\d+\]\.polylines\[\d+\]\.area,
					aggRs.*model,
					aggRs\[\d+\]\.suburbs\[\d+\]\.model,
					aggRs\[\d+\]\.suburbs\[\d+\]\.polylines\[\d+\]\.suburb,
				</param>
			</result>
			<result name="error" type="myJson">
				<param name="includeProperties">
                    actionMessages.*,
                    errorMessages.*,
				</param>
			</result>
		</action>
