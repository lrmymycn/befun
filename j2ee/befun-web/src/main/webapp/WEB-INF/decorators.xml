<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/decorators">
	<excludes>
		<pattern>/includes/*</pattern>
		<pattern>*json*</pattern>
		<pattern>*.txt*</pattern>
		<pattern>*.gif*</pattern>
		<pattern>*.jpg*</pattern>
		<pattern>*.png*</pattern>
		<pattern>*.bmp*</pattern>
		<pattern>*.js*</pattern>
		<pattern>*.css*</pattern>
	</excludes>
	<decorator name="admin-list" page="admin-list.jsp">
		<pattern>/admin/data/demand*.action</pattern>
	</decorator>
	<decorator name="admin-editor" page="admin-editor.jsp">
		<pattern>/admin/data/view*.action</pattern>
		<pattern>/admin/data/edit*.action</pattern>
		<pattern>/admin/data/createPage*.action</pattern>
		<pattern>/admin/data/getById*.action</pattern>
	</decorator>
	<decorator name="admin-modify-result" page="admin-modify-result.jsp">
		<pattern>/admin/data/save*.action</pattern>
		<pattern>/admin/data/remove*.action</pattern>
		<pattern>/admin/data/enable*.action</pattern>
		<pattern>/admin/data/disable*.action</pattern>
	</decorator>
	<decorator name="admin-profile-list" page="admin-profile-list.jsp">
		<pattern>/admin/profile/demand*.action</pattern>
	</decorator>
    <!--  -->
    <decorator name="admin-profile-editor" page="admin-profile-editor.jsp">
        <pattern>/admin/profile/assignRolesPage.action</pattern>
        <pattern>/admin/profile/view*.action</pattern>
        <pattern>/admin/profile/edit*.action</pattern>
        <pattern>/admin/profile/createPage*.action</pattern>
    </decorator>
	<decorator name="admin-user-result" page="admin-modify-result.jsp">
		<pattern>/admin/user/createProfile.action</pattern>
		<pattern>/admin/user/changePassword.action</pattern>
		<pattern>/admin/user/createClient.action</pattern>
		<pattern>/admin/user/assignRoles.action</pattern>
	</decorator>
</decorators>